{
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": ".",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "moduleNameMapper": {
        "^@/(.*)$": "<rootDir>/src/$1",
        "^@utils/(.*)$": "<rootDir>/src/common/utils/$1"
    },
    "testEnvironment": "./prisma/prisma-test-environment.ts"
}
